{
    "version": 2,
    "crons": [{
        "path": "./api/cron",
        "schedule": "*/30 * * * *"
      }],
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}